        if (cropY2 &lt; cropY1)
        {
            _tmp = cropY2;
            cropY2 = cropY1;
            cropY1 = _tmp;
        }
    }
}
else if (mouse_check_button(mb_left) &amp;&amp; cropping)
{
    cropX2 = lx;
    cropY2 = ly;
}
