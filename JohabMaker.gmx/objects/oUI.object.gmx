            charSelected = _mx + _my * gridWid;
            build_char_surface_preview(charSelected);
            
            cropX1 = 0;
            cropY1 = 0;
            cropX2 = 0;
            cropY2 = 0;
        }
    }
}
else if (mouse_check_button_released(mb_left))
{
    if (cropping)
    {
        cropping = false;
        
        var _tmp;
        if (cropX2 &lt; cropX1)
        {
            _tmp = cropX2;
            cropX2 = cropX1;
            cropX1 = _tmp;
